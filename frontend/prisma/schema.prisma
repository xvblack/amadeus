// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgres"
  url      = env("AMADEUS_POSTGRES_DATABASE_URL")
}

model Post {
  id         Int      @id @default(autoincrement())
  url        String
  time_added Int
  source     String
  tags       String[]
  attrs      Json
  links      Json
  title      String?
  abstract   String?
  content    String?
  html       String?

  @@unique([source, url])
}
